    "module": "es2020",
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es2017",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
